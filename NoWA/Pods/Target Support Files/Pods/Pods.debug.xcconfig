CLANG_CXX_LANGUAGE_STANDARD = c++11
CLANG_CXX_LIBRARY = libc++
GCC_PREPROCESSOR_DEFINITIONS = $(inherited) COCOAPODS=1
HEADER_SEARCH_PATHS = $(inherited) "${PODS_ROOT}/Headers/Public" "${PODS_ROOT}/Headers/Public/AFNetworking" "${PODS_ROOT}/Headers/Public/PureLayout" "${PODS_ROOT}/Headers/Public/RestKit" "${PODS_ROOT}/Headers/Public/RestKit/RestKit" "${PODS_ROOT}/Headers/Public/RestKit/RestKit/CoreData" "${PODS_ROOT}/Headers/Public/RestKit/RestKit/Network" "${PODS_ROOT}/Headers/Public/RestKit/RestKit/ObjectMapping" "${PODS_ROOT}/Headers/Public/RestKit/RestKit/Support" "${PODS_ROOT}/Headers/Public/SOCKit" "${PODS_ROOT}/Headers/Public/TransitionKit" "${PODS_ROOT}/Headers/Public/pop"
OTHER_CFLAGS = $(inherited) -isystem "${PODS_ROOT}/Headers/Public" -isystem "${PODS_ROOT}/Headers/Public/AFNetworking" -isystem "${PODS_ROOT}/Headers/Public/PureLayout" -isystem "${PODS_ROOT}/Headers/Public/RestKit" -isystem "${PODS_ROOT}/Headers/Public/RestKit/RestKit" -isystem "${PODS_ROOT}/Headers/Public/RestKit/RestKit/CoreData" -isystem "${PODS_ROOT}/Headers/Public/RestKit/RestKit/Network" -isystem "${PODS_ROOT}/Headers/Public/RestKit/RestKit/ObjectMapping" -isystem "${PODS_ROOT}/Headers/Public/RestKit/RestKit/Support" -isystem "${PODS_ROOT}/Headers/Public/SOCKit" -isystem "${PODS_ROOT}/Headers/Public/TransitionKit" -isystem "${PODS_ROOT}/Headers/Public/pop"
OTHER_LDFLAGS = $(inherited) -ObjC -l"AFNetworking" -l"PureLayout" -l"RestKit" -l"SOCKit" -l"TransitionKit" -l"c++" -l"pop" -framework "CFNetwork" -framework "CoreData" -framework "CoreGraphics" -framework "MobileCoreServices" -framework "Security" -framework "SystemConfiguration"
PODS_ROOT = ${SRCROOT}/Pods